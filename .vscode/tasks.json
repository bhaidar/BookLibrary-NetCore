{
    "version": "0.1.0",
    "command": "dotnet",
    "isShellCommand": true,
    "args": [],
    "tasks": [
        {
            "taskName": "build",
            "args": [
                "${workspaceRoot}/BookLibrary-NetCore.sln"
            ],
            "isBuildCommand": true,
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "xunit",
            "args": ["-verbose"],
            "isBuildCommand": false,
            "isTestCommand": true,
            "problemMatcher":"$msCompile",
            "options": {
                "cwd": "${workspaceRoot}/BookLibrary.Test"
            }
        }
    ]
}